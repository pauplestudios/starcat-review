{
	"require": {
		"ext-gd": "*",
		"phpmailer/phpmailer": "^6.1"
	},
	"extra": {
		"installer-paths": {
			"artifacts/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"includes/lib/{$name}/": [
				"vendor/pauple/package"
			]
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type": "git",
			"url": "https://bitbucket.org/pauple/upgrader.git"
		},
		{
			"type": "git",
			"url": "https://bitbucket.org/pauple/widgetry.git"
		},
		{
			"type": "git",
			"url": "https://bitbucket.org/pauple/starcat-review-cpt.git"
		},
		{
			"type": "git",
			"url": "https://bitbucket.org/pauple/starcat-review-comparison-table.git"
		},
		{
			"type": "git",
			"url": "https://bitbucket.org/pauple/starcat-review-woo-notification.git"
		},
		{
			"type": "git",
			"url": "https://bitbucket.org/pauple/starcat-reviews-photo-reviews.git"
		}
	],
	"require-dev": {
		"pauple/upgrader": "dev-develop",
		"pauple/widgetry": "dev-develop",
		"pauple/starcat-review-woo-notify": "dev-release/0.2.1",
		"pauple/starcat-review-cpt": "dev-release/0.3.2",
		"pauple/starcat-review-ct": "dev-release/0.2.1",
		"pauple/starcat-review-photo-reviews": "dev-release/0.1.3",
		"lucatume/wp-browser": "~2.1.5",
		"bmitch/churn-php": "^1.0",
		"sebastian/phpcpd": "^4.1",
		"wpackagist-plugin/woocommerce": "4.4.1",
		"wpackagist-plugin/wp-migrate-db": "1.0.15"
	}
}