{
    "name": "starcat-review",
    "version": "0.5",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build_semantic": "cd includes/assets/vendors/semantic && gulp build && cd ../../../../",
        "build_addons": "chmod +x addons.sh && bash addons.sh",  
        "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --mode=production",
        "start": "./node_modules/.bin/webpack --mode=development --watch",
        "buildtest": "cross-env NODE_ENV=test ./node_modules/.bin/webpack",
        "lint": "esw webpack.config.* src tools --color",
        "runtest": "docker-compose down && docker-compose up -d && sleep 20 && yarn acceptance",
        "acceptance": "vendor/bin/codecept run acceptance --steps",
        "acceptance_spec": "vendor/bin/codecept run acceptance tests/acceptance/BasicCest.php:comparisonTableAddonActivation --steps",
        "unittest": "yarn build && docker-compose down && docker-compose up",
        "wpunit": "vendor/bin/codecept run wpunit --steps",
        "wpunit_spec": "vendor/bin/codecept run wpunit tests/wpunit/NotificationTest.php -vvv",
        "runtest2": "yarn build && docker-compose down && docker-compose up -d && sleep 10 && yarn codeceptjs",
        "analyse": "vendor/bin/churn run includes features utils && vendor/bin/phpcpd includes features utils",
        "rename_old" : "renamer --find artifacts/addons/dist --replace starcat-review-cpt --dry-run",
        "test-wpunit": "docker-compose exec ./vendor/bin/codecept run wpunit"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@wordpress/babel-plugin-makepot": "^2.1.2",
        "babel-cli": "^6.26.0",
        "babel-jest": "^23.4.2",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "base-64": "^0.1.0",
        "bestzip": "^2.1.5",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "clean-webpack-plugin": "^0.1.16",
        "codeceptjs": "^2.3.0",
        "copyfiles": "^2.2.0",
        "cpy-cli": "^3.1.0",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-watch": "^4.0.2",
        "file-loader": "^2.0.0",
        "filemanager-webpack-plugin": "^2.0.5",
        "install": "^0.12.2",
        "mini-css-extract-plugin": "^0.4.4",
        "node-sass": "4.13.0",
        "npm": "^6.11.2",
        "puppeteer": "^1.19.0",
        "renamer": "^2.0.0",
        "resolve-url-loader": "^3.1.0",
        "sass-loader": "^7.3.1",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^2.1.3",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.7",
        "webpack-dashboard": "^2.0.0",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-dev-server": "^3.8.0",
        "webpack-hot-middleware": "^2.25.0"
    },
    "dependencies": {
        "@babel/preset-env": "^7.5.5",
