  "scripts": {
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --mode=production",
    "start": "./node_modules/.bin/webpack --mode=development --watch",
    "buildtest": "cross-env NODE_ENV=test ./node_modules/.bin/webpack",
    "lint": "esw webpack.config.* src tools --color",
    "acceptance": "vendor/bin/codecept run acceptance -vvv",
    "wpunit": "vendor/bin/codecept run wpunit -vvv",
    "runtest": "yarn build && docker-compose down && docker-compose up -d && sleep 10 && yarn acceptance",
    "runtest2": "yarn build && docker-compose down && docker-compose up -d && sleep 10 && yarn codeceptjs",
    "unittest": "yarn build && docker-compose down && docker-compose up -d && sleep 10 && yarn wpunit",
    "analyse": "vendor/bin/churn run includes features utils && vendor/bin/phpcpd includes features utils"
  },
